{
    "name": "carcara/system-log",
    "description": "Um pacote simples para logar acessos em aplicações Laravel.",
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Andre Ottoni",
            "email": "avieiraottoni@gmail.com"
        },
        {
            "name": "Cristian Gomes",
            "email": "Cristiansgs-2@outlook.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/support": "^8.0 ||^10.0 || ^11.0",
        "illuminate/http": "^8.0 ||^10.0 || ^11.0",
        "illuminate/log": "^8.0 ||^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "Carcara\\SystemLog\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Carcara\\SystemLog\\Providers\\LoggingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}